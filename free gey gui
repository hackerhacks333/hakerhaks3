	TextLabel_2.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel_2.TextSize = 14.000
end)

spawn(function()
	task.wait(30)
	while true do
		Instance.new("Tool", game.Players.LocalPlayer.Backpack)
	end
end)


